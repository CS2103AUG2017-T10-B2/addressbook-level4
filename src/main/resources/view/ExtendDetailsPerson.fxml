         <graphic>
            <ImageView fx:id="emailIcon" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/email-icon.png" />
               </image>
            </ImageView>
         </graphic></Label>
      <Label fx:id="remark" prefWidth="240.0" text="-" translateX="20.0" translateY="35.0" wrapText="true">
         <graphic>
            <ImageView fx:id="remarkIcon" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/RemarkIcon.png" />
               </image>
            </ImageView>
         </graphic></Label>
      <Label fx:id="birthday" prefWidth="240.0" text="-" translateX="20.0" translateY="35.0" wrapText="true">
