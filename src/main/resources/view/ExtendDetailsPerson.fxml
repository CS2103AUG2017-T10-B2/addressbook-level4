      <Label fx:id="age" prefWidth="240.0" text="-" translateX="20.0" translateY="40.0">
         <graphic>
            <ImageView fx:id="ageIcon" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/AgeIcon.png" />
               </image>
            </ImageView>
         </graphic></Label>
